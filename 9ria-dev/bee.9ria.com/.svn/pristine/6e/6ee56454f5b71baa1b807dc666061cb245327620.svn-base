body {
  background-color: #f2f2f2;
  /*min-height: 2000px;*/
  padding-top: 70px;
  font: \5FAE\8F6F\96C5\9ED 14px/1.2em;/*微软雅黑*/
  color: #3e3e3e;
}
ul{list-style: none;}
li{margin: 0;padding: 0;}
.navbar-default{background-color:#1e1e1e}
.navbar-default .navbar-nav>.active>a, .navbar-default .navbar-nav>.active>a:focus, .navbar-default .navbar-nav>.active>a:hover,.navbar-default .navbar-nav>li>a:focus, .navbar-default .navbar-nav>li>a:hover{background-color: #000000}
.navbar-default .navbar-nav>.active>a, .navbar-default .navbar-nav>.active>a:focus, .navbar-default .navbar-nav>.active>a:hover,.navbar-default .navbar-nav>li>a:focus, .navbar-default .navbar-nav>li>a:hover,.navbar-default .navbar-nav>li>a{font-size:18px;color: #099bee;height: 60px;line-height: 30px;padding: 15px 50px;font-weight: bold;}
.navbar-default .navbar-nav>li>a{color: #fff}
.navbar{min-height: 60px}
.navbar-brand,.navbar-nav>li>a{height: 60px;line-height: 30px}
.navbar a.logo{float:left;background:url(../images/logo.png)  0 15px no-repeat;display: inline-block;width: 154px;height: 60px;margin-left: 40px}
.loginInfo{float: right;margin-right: 40px;height: 60px;line-height: 60px;}
.loginInfo a{color: #fff;font-size: 18px}
/*.navbar-nav>li{width: 200px;text-align: center;}*/


.container{/*border: #ccc solid 1px*/position: relative;}
.crumb{font-size: 24px;font-weight: bold;color: #099bee;padding: 15px 0 25px 0}
.crumb a,.crumb a:hover{color: #3e3e3e;text-decoration: none;}
ul.tag{position:absolute;top:66px;left:15px;z-index:10;overflow: hidden;padding: 0;margin-bottom: 20px}
/*ul.tag li li:hover{width: 100%;background-color: red;}*/
ul.tag li.active a{background-color: #099bee;color: #fff}
ul.tag li{cursor:pointer;float:left;font-size: 18px;color: #747474;background-color: #fff;/*border: #ccc solid 1px;*//*padding: 5px 10px;*/margin: 0 15px 5px 0;text-align: center;}
ul.tag li a{display: inline-block;width: 100%;text-decoration: none;color: #747474;padding: 5px 30px;}
ul.subTag{display: none;}
ul.tag li a:hover{background-color: #099bee;color: #fff}
ul.subTag{padding-left: 0}
ul.subTag li{float: none;width: 100%;text-align: center;margin:0;/*padding: 0*/}
.topB{position: relative;width: 243px}
.topB span,.topB em{display:inline-block;width:100%;height:35px;line-height:35px;position: absolute;bottom: 0;left: 0;text-align: center;}
.topB span{background-color:#000;opacity: 0.5}
.topB em{font-style: normal;color: #fff;}

ul.main{overflow: hidden;padding: 0;margin-top: 50px;}
ul.main li{position:relative;float:left;margin:0 40px 40px 0;width:243px;height: 336px;background-color: #fff;overflow: hidden;}
ul.main li img{width: 243px;height: 243px;}
.codeBox,.codeBoxTop{position:absolute;top:0;left:0;}
.codeBox{background:#000;opacity: 0.7;width: 100%;height: 100%;}
.codeBoxTop{padding: ;text-align: center;}
.codeBoxTop a{font-size:16px;color:#fff;border: #fff solid 1px;padding: 5px 19px;}
.codeBoxTop img{width: 215px!important;height: 215px!important;margin: 10px 0 44px 0}
ul.main p{padding:0 5px 5px 5px;font-size:14px;color: #868686}
ul.main h3{display:inline-block;width:233px;height:35px;line-height:35px;overflow:hidden;padding:0 5px 5px 5px;margin:0 5px;border-bottom:#868686 solid 1px;font-size:16px;color: #303030;font-weight: bold;}
a.more{display: inline-block;width: 96%;height: 40px;background-color: #000;margin-bottom:100px;color: #fff;text-align: center;font-size: 22px;line-height: 40px;}
a.more:hover{text-decoration: none;}


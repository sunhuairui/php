.btnBlue{background-color: #099bee;color: #fff;font-size:16px;padding: 5px 30px;text-align: center;height: 40px;line-height: 30px;cursor: pointer; }
a.btnBlue:hover{color: #fff;text-decoration: none;}
i.delBtnHid,i.delBtn,i.moveBtn{background: url(../images/sprite.png) no-repeat;}
i.delBtnHid{display: inline-block;width:15px;height: 15px;margin-left:5px;background-position: 0 -2024px;display: none; }
i.delBtn{display: inline-block;width: 30px;height: 30px;margin:0 10px;cursor: pointer;position: absolute;top:25px;}
i.moveBtn{display: inline-block;width: 30px;height: 30px;margin:0 10px;cursor: pointer;position: absolute;top:25px;}
i.delBtn2:hover{cursor: pointer;}
i.delBtn{background-position: 0px -1557px;right: 0px;}
i.delBtn:hover{background-position: 0px -1628px}
i.moveBtn{background-position: 0px -1696px;margin-left: 63px;right: 43px;}
i.moveBtn:hover{background-position: 0px -1759px}
.addBtn{display: inline-block;width: 30px;height: 30px;position: absolute;}



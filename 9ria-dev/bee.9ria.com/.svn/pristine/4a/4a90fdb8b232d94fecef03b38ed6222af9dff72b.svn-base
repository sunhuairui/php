/* base start */
body,div,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,pre,form,fieldset,input,textarea,p,blockquote,th,td{padding:0;margin:0;-webkit-tap-highlight-color:rgba(255,255,255,0)}
section,article,aside,header,footer,nav,dialog,figure{display:block}
ol,ul{list-style:none}
h1,h2,h3,h4,h5,h6{font-size:100%; font-weight:normal;}
img{border:0;vertical-align:middle}
input{vertical-align:middle}
table{border-collapse:collapse;border-spacing:0}
address,caption,cite,code,dfn,em,th,var,i{font-weight:normal;font-style:normal}
input[type="radio"], input[type="checkbox"], input[type="submit"], input[type="rest"], input[type="button"], button, select {cursor:pointer}
input[type="text"], input[type="number"], input[type="tel"], input[type="password"], input[type="submit"], input[type="rest"], input[type="button"], button, textarea {-webkit-border-radius:0;-webkit-appearance:none;-moz-appearance:none}
a {color:#007bed;text-decoration:none}a:visited{color:#007bed;}
a img {border:0}
html,body{font:14px/1.5 "Helvetica Neue", HelveticaNeue, "Helvetica-Neue", Helvetica, "BBAlpha Sans", sans-serif,"microsoft yahei";color:#242424;background:#FFFFFF;}
.clearfix:after {display:block;clear:both;height:0;visibility:hidden;font-size:0;line-height:0;content:'';}
.clearfix {zoom:1}
select{-webkit-appearance:none;width:99%;height:30px;padding-left:5px;/*-webkit-appearance: menulist;*/box-sizing: border-box;align-items: center;border:0 none;border-image-source: initial;border-image-slice: initial;border-image-width: initial;border-image-outset: initial;border-image-repeat: initial;white-space: pre;-webkit-rtl-ordering: logical;color: black;background-color: white;cursor: default;}
input[type="text"], input[type="number"], input[type="tel"], input[type="password"],textarea{border:1px solid #CFCFCF;}
html,body {min-height:100%;width:100%;overflow-x:hidden;}

@font-face {
	font-family: 'icomoon';
	src:url('../fonts/icomoon.eot?-d4d1cn');
	src:url('../fonts/icomoon.eot?#iefix-d4d1cn') format('embedded-opentype'),
		url('../fonts/icomoon.woff?-d4d1cn') format('woff'),
		url('../fonts/icomoon.ttf?-d4d1cn') format('truetype'),
		url('../fonts/icomoon.svg?-d4d1cn#icomoon') format('svg');
	font-weight: normal;
	font-style: normal;
}

[class^="icon-"], [class*=" icon-"] {
	font-family: 'icomoon';
	speak: none;
	font-style: normal;
	font-weight: normal;
	font-variant: normal;
	text-transform: none;
	line-height: 1;

	/* Better Font Rendering =========== */
	-webkit-font-smoothing: antialiased;
	-moz-osx-font-smoothing: grayscale;
}

.icon-bubble:before {
	content: "\34";
	margin-right:3px;
}
.icon-like:before {
	content: "\e601";
	margin-right:3px;
}
.icon-search:before {
	content: "\e602";
	margin-right:3px;
}
.icon-heart:before {
	content: "\e606";
	margin-right:3px;
}
.icon-arrow-down:before {
	content: "\e604";
	margin-right:3px;
}
.icon-clock2:before {
	content: "\e605";
	margin-right:3px;
}
.icon-play:before {
	content: "\e607";
}

.pw_16 {width:16%;}
.pw_18 {width:18%;}
.pw_20 {width:20%;}
.pw_22 {width:22%;}
.pw_28 {width:28%;}
.pw_30 {width:30%;}
.pw_32 {width:32%;}
.pw_33 {width:33.333%;}
.pw_40 {width:40%;}
.pw_45 {width:45%;}
.pw_50 {width:50%;}

.l {float:left;}
.r {float:right;}

.red_1 {color:#ff6a06;text-shadow: 1px 1px 5px rgba(161, 161, 161, 0.6470588);
-webkit-text-shadow: 1px 1px 5px rgba(161, 161, 161, 0.6470588);}
/* base end */

/* style start */
.theme-start-1 {display:inline-block;background-image: url('../images/start.png');background-size: 100% 100%;}
.theme-btn-1 {display: inline-block;background-color: #ffe62b;text-align: center;font-weight: bold;color:#424454;border-radius:5px;-webkit-border-radius:5px;}
.theme-ico-radius-1:before {width:10px;height:10px;margin-right:5px;content:'';display:inline-block;border-radius:10px;-webkit-border-radius:10px;background-color: #ff5f23;position: absolute;top:50%;margin-top:-5px;left:-15px;}
.theme-arrow-up {content:'';display:inline-block;width:0px;height:0px;border-left:8px solid transparent; border-right:8px solid transparent;border-bottom:8px solid #fff;font-size:0px;line-height:0px;}
.theme-tips-arrow-new{display:inline-block;font-size:16px;line-height:0px;-webkit-transform:rotate(45deg);transform:rotate(45deg);position:absolute;top:13px;right:1px;}
.theme-tips-arrow-new {display:block;font-size:10px;font-weight: bold;width:0px;height:0px;border-left:25px solid transparent; border-right:25px solid transparent;border-bottom:25px solid #7bd400;position:absolute;right:0;top:0;z-index:1;-webkit-transform:translate(17px,-5px) rotate(45deg);
transform:translate(17px,-5px) rotate(45deg);}
.theme-tips-arrow-new:after {content:'最新';width:25px;font-size:10px;color:#47602b;position:absolute;top:15px;left:-12px;text-align: center;}

.theme-tips-arrow-recom{display:inline-block;font-size:16px;line-height:0px;-webkit-transform:rotate(45deg);transform:rotate(45deg);position:absolute;top:13px;right:1px;}
.theme-tips-arrow-recom {display:block;font-size:10px;font-weight: bold;width:0px;height:0px;border-left:25px solid transparent; border-right:25px solid transparent;border-bottom:25px solid #f9e900;position:absolute;right:0;top:0;z-index:1;-webkit-transform:translate(17px,-5px) rotate(45deg);
transform:translate(17px,-5px) rotate(45deg);}
.theme-tips-arrow-recom:after {content:'推荐';width:25px;font-size:10px;color:#47602b;position:absolute;top:15px;left:-12px;text-align: center;}
.theme-btn-2 {display: block;width:215px;height:42px;background-color: #ffe62b;text-align: center;line-height:42px;border-radius:5px 5px 5px 5px;-webkit-border-radius:5px 5px 5px 5px;color:#424454;font-weight: bold;font-size:20px;}
/* style end */

/* pubic */
.nav {height:42px;background-color: #ffe62b;position:relative;z-index:999;margin-bottom:10px;}
.nav_ul {overflow:hidden;}
.nav_li{line-height:42px;display: inline-block;text-align: center;position:relative;float:left;overflow:hidden;}
.nav_li .btn {position:relative;}
.nav_li a {color:#515151;font-size:14px;}
.nav_li .theme-arrow-up {position:absolute;bottom:0;left:50%;margin-left:-7px;}

.sub_nav {width:90%;margin:0 auto;border:1px solid #ffe62b;border-radius:10px 10px 10px 10px;-webkit-border-radius:10px 10px 10px 10px;overflow: hidden;text-align: center;font-size:12px;margin-bottom:12px;margin-top:12px;}
.sub_nav .item {float:left;height:24px;line-height:24px;position:relative;}
.sub_nav .item:first-child {border-radius:10px 0 0 10px;-webkit-border-radius:10px 0 0 10px;}
.sub_nav .item:last-child {border-radius:0 10px 10px 0;-webkit-border-radius:0 10px 10px 0;}
.sub_nav .item:after{content:'';height:100%;border-right:1px solid #ffe62b;position:absolute;right:0;top:0;}
.sub_nav .item:last-child:after {display:none;}
.sub_nav .active {background-color: #ffe62b}
.nav_line:after {content:'';display:inline-block;position:absolute;height:16px;width:0;border-left:1px solid #ada884;top:50%;right:0;margin-top:-8px;}
.nav_more .icon-arrow-down {margin-left:5px;display:inline-block;}
.sub_nav a {color:#2c2d34;}


.gd_head {background-color: #f2f2f2;padding-top:5px;}
.d_infor {overflow:hidden;padding-top:5px;}
.d_infor dt {float:left;width:16%;}
.d_infor dt a {display:block;width:40px;height:40px;margin:10px auto 0 auto;}
.d_infor dt img {width:40;height:40px;}

.d_infor dd {margin-left:16%;color:#797979;}
.d_infor dd h3 {line-height:18px;position:relative;vertical-align: middle;margin-bottom:5px;}
.d_infor dd .tit {color:#5a5b5b;display:inline-block;overflow:hidden;height:18px;vertical-align: middle;font-size:18px;text-shadow: 1px 1px 5px rgba(161, 161, 161, 0.6470588);
-webkit-text-shadow: 1px 1px 5px rgba(161, 161, 161, 0.6470588);}
.d_infor dd .starts {width:50%;display:inline-block;text-align: center;vertical-align: middle;}
.d_infor dd .theme-start-1 {width:14px;height:14px;}
.d_infor dd .ico-new {display: inline-block;position: absolute;top:3px;right:0;width:30px;height:10px;background-color: #f1e05b;color:#3d3a2f;font-size:8px;line-height:10px;text-align:center;}
.d_infor dd .tips {font-size:11px;margin-bottom:5px;}
.d_infor dd .tips p {height:20px;}
.d_infor dd .tips .t_box {display:inline-block;overflow: hidden;white-space:nowrap;margin-right:3px;}
.d_infor dd .infor {font-size:14px;color:#797979;line-height:18px;margin-bottom:5px;}

.interact {font-size:10px;height:24px;color:#797979;text-align: center;}
.interact .ico {width:65px;display:inline-block;}
.content {padding-bottom:10px;}

.white_bg {background-color: #fff}

/* 游戏首页 */
#Games {background-color:#fffdf0;}
.g_list {width:93%;margin:0 auto;padding-bottom:10px;}
.game_infor {overflow:hidden;padding-top:5px;}
.game_infor dt {float:left;width:20%;}
.game_infor dt a {display:block;width:55px;height:55px;margin-top:10px;border:1px solid #737471;border-radius:10px;-webkit-border-radius:10px;box-shadow: 0px 5px 2px #aaaaaa;-webkit-box-shadow: 0px 1px 1px #aaaaaa;margin:10px auto 0 auto;}
.game_infor dt img {width:55;height:55px;}

.game_infor dd {margin-left:22%;color:#797979;}
.game_infor dd h3 {line-height:18px;position:relative;vertical-align: middle;}
.game_infor dd .tit {color:#5a5b5b;display:inline-block;overflow:hidden;height:18px;vertical-align: middle;font-size:16px;text-shadow: 1px 1px 5px rgba(161, 161, 161, 0.6470588);
-webkit-text-shadow: 1px 1px 5px rgba(161, 161, 161, 0.6470588);}
.game_infor dd .starts {width:50%;display:inline-block;text-align: center;vertical-align: middle;}
.game_infor dd .starts em {width:17px;height:17px;}
.game_infor dd .ico-new {display: inline-block;position: absolute;top:3px;right:0;width:30px;height:10px;background-color: #f1e05b;color:#3d3a2f;font-size:8px;line-height:10px;text-align:center;}

.game_infor dd .tips {font-size:11px;margin-bottom:5px;}
.game_infor dd .infor {font-size:14px;color:#797979;line-height:18px;margin-bottom:5px;}

.game_infor dd .btns {font-size:10px;height:24px;}
.game_infor dd .btns .l {display:inline-block;height:24px;line-height:24px;}
.game_infor dd .btns .l span {margin-right:8px;}
.game_infor dd .btns .ico {margin-right:3px;}
.game_infor dd .btns .ico:before {margin-right: 3px;}
.game_infor dd .btns .play {width:72px;height:24px;line-height:24px;}

.game_infor:after {content:'';display:block;border-bottom:1px dashed #d9d8d0;height:0;overflow:hidden;width:95%;margin:4px auto 0 auto;}
.game_infor:last-of-type:after {content: '';display:none;}


#Games .red {color:#f84b29;text-shadow: 1px 1px 5px rgba(161, 161, 161, 0.6470588);
-webkit-text-shadow: 1px 1px 5px rgba(161, 161, 161, 0.6470588);font-weight: bold;}

/*游戏 detail*/

.photos {width:100%;height:200px;background-color: #e3e3e3;padding:5px 0;overflow:hidden;}
.photos .cont li {float:left;height:200px;text-align:center;line-height:200px;width:100%;}
.photos .cont li img {vertical-align: middle;width:300px;height:200px;max-width: 300px;max-height:200px;}

.photos .btns {height:24px;width:100%;position:absolute;z-index:2;left:0;bottom:0;text-align: center;padding-bottom:0px;}
.photos .btns li {vertical-align:middle;display: inline-block;width:10px;height:10px;background-color: #fff;box-shadow: 0px 10px 5px rgba(161, 161, 161, 1);-webkit-box-shadow:0px 2px 2px rgba(161, 161, 161, 1);border-radius:10px 10px 10px 10px;-webkit-border-radius:10px 10px 10px 10px;margin:0 8px;}
.photos .btns li.on {width:12px;height:12px;border:1px solid #fff;background-color: #6eb5e5;box-shadow: 0px 10px 5px rgba(161, 161, 161, 1);-webkit-box-shadow:0px 2px 2px rgba(161, 161, 161, 1);border-radius:12px 12px 12px 12px;-webkit-border-radius:12px 12px 12px 12px;}
.photos .btns:after {content:'';display:block;height:24px;position: absolute;left:0;bottom:0;width:100%;background-color:#fff;opacity: 0.3;z-index:1;}

.gd_detail {margin:0 auto;width:95%;padding:10px 0;color:#797979;line-height:20px;font-size:14px;overflow:hidden;}
.gd_detail p {margin-bottom:16px;}
.gd_detail img {max-width: 100%}

.gd_play {
  margin:0 auto;
}

/*learning*/
.learning_page_cont {margin:0 4px;}
.learning_list {overflow:hidden;margin:0 auto;}
.learning_list .box {width:50%;float:left;margin-bottom:6px;}
.learning_list .child {width:148px;height:162px;margin:0 auto;border:1px solid #e3e3e3;background-color: #f7f7f7;}
.learning_list .child .hd {position:relative;}
.learning_list .child .hd img {width:148px;height:80px;border-bottom:1px solid #e8eaf1;}
.learning_list .child .hd .tit {position:absolute;left:0;top:34px;width:100%;height:31px;color:#fff;overflow:hidden;}
.learning_list .child .hd .tit span {display:block;position:absolute;left:0;top:0;z-index:1;width:100%;height:29px;background:#000;border-top:1px solid #333;border-bottom:1px solid #333;opacity: 0.5}
.learning_list .child .hd .tit em {color:#f3f3f3;font-weight:200;display:block;position:relative;z-index:2;font-size:12px;line-height:12px;padding-left:7px;text-shadow: 0px 1px 1px #333;-webkit-text-shadow: 0px 1px 1px #333;}
.learning_list .child .hd .tit strong {display:block;position:relative;z-index:2;font-size:14px;padding-left:5px;text-shadow: 0px 2px 1px #3c3c3c;-webkit-text-shadow: 0px 2px 1px #3c3c3c;}
.learning_list .child .hd a {position:absolute;top:0;left:0;z-index:5;width:100%;height:100%;}
.learning_list .child .md {text-align:center;}
.learning_list .child .md .name a{color:#454545;font-size:14px;text-shadow: 0px 0px 3px #454545;-webkit-text-shadow: 0px 0px 3px #454545;}
.learning_list .child .md .infor {color:#a3a3a3;font-size:10px;overflow: hidden;line-height:20px;height:20px;}
.learning_list .child .md .infor .icon-clock2:before {margin-right: 3px}
.learning_list .child .md .infor .theme-start-1 {width:10px;height:10px;}
.learning_list .child .ft {font-size:10px;color:#a3a3a3; }
.learning_list .child .ft span {display: inline-block;width:31%;text-indent: 5px}

/*learning detail*/
.l_detail {background-color: #fff;padding-top:1px;padding-bottom:10px;margin:10px;}
.video {min-width:310px;min-height:180px;margin:8px auto 14px auto;}
.l_detail #video {min-width:310px;min-height:180px;margin:0 auto;display: block;}
.video_list {border:1px solid #e6e6e6;margin:0 5%;}
.video_list dt {height:25px;line-height:25px;text-indent: 10px;color:#828282;font-size:14px;border-bottom:1px solid #e6e6e6;}
.video_list dd {min-height:120px;padding:12px;}
.video_list dd ol {border-left:1px solid #8bb8ef;}
.video_list dd ol li {position: relative;margin:0 16px;}
.video_list dd ol li:before {content: '';display: block;width:14px;height:14px;border:1px solid #bcbcbc;border-radius:14px;-webkit-border-radius:14px;position:absolute;left:-24px;top:8px;}
.video_list dd ol li:after {content:'';display:block;clear: both;height:0;line-height:0;border-bottom:1px dashed #b6b6b6;padding-top:5px;}
.video_list dd ol li .infor {float:left;width:70%;line-height:24px;margin-right:5px;}
.video_list dd ol li .infor h4 {padding-top:5px;}
.video_list dd ol li .infor h4 a{color:#545454;font-size:14px;}
.video_list dd ol li .infor p {font-size:10px;color:#aea5a4;}
.video_list dd ol li .action {float:left;width:25%;padding-top:5px;}
.video_list dd ol li .action .time {color:#545454;font-size:12px;line-height:24px;vertical-align: middle;}
.video_list dd ol li .action .icon-play {position:relative;display:inline-block;width:18px;height:12px;vertical-align: middle;}
.video_list dd ol li .action .icon-play:before {position:absolute;left:0;top:0;color:#555;}

/* event */
.e_list {margin: 0 6px;}
.e_list ul{max-width:980px;margin:0 auto;overflow:hidden;}
.e_list ul .box {float:left;margin-bottom:12px;}
.e_list .box_cont {width:306px;height:207px;border:1px solid #e6e6e6;margin:0 auto;}
.e_list .box_cont .hd {border-bottom:1px solid #e6e6e6;margin-bottom:3px;}
.e_list .box_cont .hd img {width:306px;height:160px;}
.e_list .box_cont .tit {line-height:22px;overflow:hidden;text-indent: 10px}
.e_list .box_cont .tit a {font-size:14px;color:#515151;}
.e_list .box_cont p {font-size:12px;color:#949494;text-indent: 10px}


/* event detail */
.page_hd {height:70px;}
.page_hd .tit {text-align:center;padding-top:8px;font-weight: bold;color:#5e5e5e;}
.page_hd .sub_tit {color:#797979;font-size: 12px;padding-top:10px;text-align: center;}
.page_hd .sub_tit span {display: inline-block;}

@media screen and (min-width: 320px) {
  .learning_list .box {width:50%;}
  .l_detail #video,.video {width:310px;height:180px;}
}

@media screen and (min-width: 550px) {
  .learning_list .box {width:33%;}
  .l_detail #video,.video {width:310px;height:180px;}
}

@media screen and (max-width: 650px) {
  .e_list ul .box {width:100%;}
}

@media screen and (min-width: 650px) {
  .e_list ul .box {width:50%;}
}

@media screen and (min-width: 900px) {
  .learning_list .box {width:25%;}
  .l_detail #video,.video {width:726px;height:456px;}
}

@media screen and (min-width: 930px) {
  .e_list ul .box {width:33.3333%;}
}

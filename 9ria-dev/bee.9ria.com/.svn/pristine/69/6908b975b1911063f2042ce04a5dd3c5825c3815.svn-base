.swiper-container{margin:0 auto;position:relative;overflow:hidden;-webkit-backface-visibility:hidden;-moz-backface-visibility:hidden;-ms-backface-visibility:hidden;-o-backface-visibility:hidden;backface-visibility:hidden;z-index:1;}.swiper-wrapper{position:relative;width:100%;-webkit-transition-property:-webkit-transform,left,top;-webkit-transition-duration:0s;-webkit-transform:translate3d(0px,0,0);-webkit-transition-timing-function:ease;-moz-transition-property:-moz-transform,left,top;-moz-transition-duration:0s;-moz-transform:translate3d(0px,0,0);-moz-transition-timing-function:ease;-o-transition-property:-o-transform,left,top;-o-transition-duration:0s;-o-transform:translate3d(0px,0,0);-o-transition-timing-function:ease;-o-transform:translate(0px,0px);-ms-transition-property:-ms-transform,left,top;-ms-transition-duration:0s;-ms-transform:translate3d(0px,0,0);-ms-transition-timing-function:ease;transition-property:transform,left,top;transition-duration:0s;transform:translate3d(0px,0,0);transition-timing-function:ease;}.swiper-free-mode>.swiper-wrapper{-webkit-transition-timing-function:ease-out;-moz-transition-timing-function:ease-out;-ms-transition-timing-function:ease-out;-o-transition-timing-function:ease-out;transition-timing-function:ease-out;margin:0 auto;}.swiper-slide{float:left;}.swiper-wp8-horizontal{-ms-touch-action:pan-y;}.swiper-wp8-vertical{-ms-touch-action:pan-x;}.swiper-container{}.swiper-slide{}.swiper-slide-active{}.swiper-slide-visible{}.swiper-pagination-switch{}.swiper-active-switch{}.swiper-visible-switch{}
body {min-width:320px;font-family:"Microsoft Yahei" !important;}
body *:active, body *:focus {outline: none;}
img {max-width:100%;max-height:100%;}
#index ul {list-style: none;padding:0px;margin:0px;}
#index .navbar {height: 74px;background: #fff;border-bottom: none;padding:0px 0px;position: relative;z-index: 9;background: #fff;}
#index .navbar .navbar-header {padding-left: 30px;height: 73px;}
#navlist {background: #fff;}
#index .navbar .navbar-brand {padding:0px;height:40px;margin-top: 15px;}
#index .navbar-nav {height: 73px;padding:12px 0px;}
#index .navbar-nav > li {margin-left: 90px;}
#index .navbar-nav > li span {display: none;}
#index .navbar-nav > li a {font-size: 24px;font-family:"Microsoft Yahei";}
#index .navbar-nav > li.active span {display: inline-block;}
#index .navbar-default .navbar-nav a, #index .navbar-default .navbar-nav > .active > a {background:none;box-shadow: none;}
#index .navbar-default .navbar-nav a:hover, #index .navbar-default .navbar-nav > .active > a {background:none;color:#3b87d5;}
#index .navbar {margin-bottom: 0px;}
#index .navbar-toggle {top:12px;}
.tit {padding:20px 0px;text-align: center;margin-top: 50px;background:url(../images/bo.jpg) repeat-x left center;}
.wan {margin-top: 30px;}
.wan li {display: inline-block;width:25%;float: left;}
.wan li.l1 {text-align: left;}
.wan li.l2 {width:50%;text-align: center;}
.wan li.l3 {text-align: right;}
.swiper {overflow: hidden;position: relative;}
.swiper .pagination {position: absolute;right:20px;top:200px;z-index: 8;}
.swiper .pagination span {width: 30px;height: 30px;border-radius: 15px;margin-top: 20px;background: #fff;display: block;cursor: pointer;opacity: 0.5;}
.swiper .pagination span.swiper-active-switch {opacity: 1;}
.swiper-slide {float: left;}
.link {text-align: center;margin-top: 10px;}
.ul li {float: left;width:25%;text-align: center;padding:30px 0px;}
.swiper {height: 605px;}
.yue {text-align: center;margin-top: 20px;}
.footer {background: #1b1b1b;padding:20px;text-align: center;margin-top: 50px;}
.row {text-align: center;}
.row > div {padding:20px 0px;}
.imglist img {margin:20px 0px;}
.imglist p {font-size: 36px;color:#9e9e9e;padding-top: 170px;line-height: 50px;text-align: left;}
.block1 p {font-size: 20px;color:#9e9e9e;padding-top: 20px;}
.block1 img {height: 192px;}
.block3 p {width: 180px;margin:0 auto;font-size: 20px;color:#9e9e9e;line-height: 30px;padding-top: 20px;padding-bottom: 20px;}

@media (max-width: 767px) {
  	#index .navbar-nav > li {margin-left: 0px;}
  	#index .navbar-nav > li.active span {display: none;}
  	#index #navlist {background: #eee;}
  	#index .navbar-nav > li a {font-size:18px;}
  	#index .navbar .navbar-header {padding-left: 10px;} 
  	#index .navbar .navbar-brand {height: 30px;margin-top: 20px;}
  	.tit img {height: 26px;}
  	.tit {margin-top: 10px;}
  	.row .col-lg-4 {padding:10px 0px;}
  	.row .col-lg-3 {width:50%;float: left;padding:10px 0px;}
  	.yue img {width: 60px;}
  	.footer {padding:20px 50px;margin-top: 30px;}
  	.imglist img {margin:0px 0px;width:80%;}
  	.imglist div {padding:10px 0px;}
  	.imglist p {font-size: 18px;text-align: center;line-height: 30px;padding-top: 0px;margin-top: -5px;}
  	.swiper {height: 160px;}
  	.swiper .pagination {right:10px;top:35px;}
  	.swiper .pagination span {width:10px;height: 10px;margin-top: 10px;}
  	#navlist {width:200px;right:15px;position: absolute;top:74px;display: none;}
    .navbar .navbar-toggle {display: none;}
  	#index #navlist {background:rgba(0,0,0,0.8);border-radius: 10px;border:none;overflow:visible;}
  	#index .navbar-default .navbar-nav a {color:#ddd;}
	.navbar-brand {width: 100%;height: 100%;text-align: center;margin-left: 0px;}
	.navbar-brand img {display: block;margin:0 auto;}
  	.block3 p {width: 140px;margin:0 auto;font-size: 14px;color:#9e9e9e;line-height: 20px;padding-top: 10px;padding-bottom: 10px;height: 80px;overflow: hidden;}
  	.block3 img {height: 100px;}
}
@media (min-width: 768px) and (max-width: 991px) {
  header {overflow: hidden;}
}



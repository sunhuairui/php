/*reset*/
ul {
	list-style: none;
}

em {
	font-style: normal
}

html,body,ul,li,ol,h1,span,em,i,div,h2,p,a {
	margin: 0;
	padding: 0;
}

.clearfix:after,.content:after {
	content: '\20';
	display: block;
	height: 0;
	clear: both
}

.clearfix:after,.content:after {
	*zoom: 1
}

.clear {
	clear: both;
	width: 1px;
	height: 0;
	font-size: 1px;
	visibility: hidden;
	overflow: hidden
}

.clearfix:after {
	visibility: hidden;
	display: block;
	font-size: 0;
	content: " ";
	clear: both;
	height: 0;
}

* html .clearfix {
	zoom: 1;
} /* IE6 */
*:first-child+html .clearfix {
	zoom: 1;
} /* IE7 */

